{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1"
    },
    {
      "src": "/admin/(.*)",
      "status": 200,
      "dest": "/admin/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}